---
import Project  from '~/components/Project.astro';
import { projectsData } from '~/data';
import Page from '~/components/Page.astro';
---

<Page>
    {
      projectsData['consulting'].map((project => (
        <>
        <ConsultingOrAnalyticsProject {...project} />
        <br><br>
        </>
      ))
    }
    {
      projectsData['analytics'].map((project) => (
        <>
        <ConsultingOrAnalyticsProject {...project} />
        <br><br>
        </>
      ))
    }
    {
      projectsData['software-engineering'].map((project) => (
        <>
        <SoftwareEngineeringProject {...project} />
        <br><br>
        </>
      ))
    }
    <br>
</Page>

<br> <br>
