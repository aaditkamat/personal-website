---
import SoftwareEngineeringProject  from '~/components/SoftwareEngineeringProject.astro';
import ConsultingOrAnalyticsProject from '~/components/ConsultingOrAnalyticsProject.astro';
import { projectsData } from '~/data';
import Page from '~/components/Page.astro';
---

<Page>
    <pre>
    {
      `projectsData['consulting'].map((project => (
        <>
        <ConsultingOrAnalyticsProject {...project} />
        <br><br>
        </>
      ));\nprojectsData['analytics'].map((project) => (
        <>
        <ConsultingOrAnalyticsProject {...project} />
        <br><br>
        </>
      ));\nprojectsData['software-engineering'].map((project) => (
        <>
        <SoftwareEngineeringProject {...project} />
        <br><br>
        </>
      ))`
    }
    </pre>
</Page>

<br> <br>
