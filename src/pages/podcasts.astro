---
import IFrame from "~/components/ui/IFrame.astro";
import Page from "~/components/common/Page.astro";
import { podcastData } from "~/data";
---

<Page title="Podcasts" headline="My Brain's Podcast Binge">
    {
        podcastData.map((podcast) => {
            return (
                <IFrame
                    className="rounded-xl ml-2 mb-8 w-full"
                    height={232}
                    {...podcast}
                />
            );
        })
    }
</Page>
