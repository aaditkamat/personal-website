---
import IFrame from '~/components/IFrame.astro';
import Page from '~/components/Page.astro';
import { podcastData } from '~/data';
---

<Page>
    {
        podcastData.map((podcast) => {
            return (
                <IFrame
                    className = 'rounded-xl mb-4 w-full'
                    height={232}
                    {...podcast} />
            )
        })
    }
</Page>
